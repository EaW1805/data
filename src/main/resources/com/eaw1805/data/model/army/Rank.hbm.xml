<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.eaw1805.data.model.army.Rank" table="TBL_RANKS">
        <cache usage="read-only"/>
        <id name="rankId" column="RANK_ID">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="minComC"/>
        <property name="maxComC"/>
        <property name="strC"/>
        <property name="salary"/>
    </class>
</hibernate-mapping>