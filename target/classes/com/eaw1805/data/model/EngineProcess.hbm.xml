<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.eaw1805.data.model.EngineProcess" table="ENGINE">
        <id name="procId" column="PROC_ID">
            <generator class="native"/>
        </id>
        <property name="gameId" column="GAME_ID"/>
        <property name="scenarioId"/>
        <property name="turn"/>
        <property name="dateStart"/>
        <property name="duration"/>
        <property name="processName"/>
        <property name="buildNumber"/>
    </class>
</hibernate-mapping>